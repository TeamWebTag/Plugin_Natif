{
  "manifest_version" : 2,
  "name" : "WEBTAG",
  "description" : "View and draw art on the web",
  "version" : "1.0",
  "permissions" : [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
    "web_accessible_resources": [
      "assets/css/*",
     "assets/js/*",
     "assets/fonts/*",
    "assets/img/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action" : {
    "default_icon" : "logo.png",
    "default_popup" : "index.html"
  },
  "icons" : {
    "256" : "logo.png"
  }
}