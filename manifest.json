{
  "manifest_version" : 2,
  "name" : "WEBTAG",
  "description" : "View and draw art on the web",
  "version" : "1.0",
  "permissions" : [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
    "web_accessible_resources": [
    "img/lol.png",
    "img/trash.png",
    "fabric.js/dist/fabric.js"  
  ],
  "browser_action" : {
    "default_icon" : "img/logo.png",
    "default_popup" : "popup.html"
  },
  "icons" : {
    "256" : "img/logo.png"
  },
   "content_scripts" : [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["./js/tag_the_web.js"]
  }]
}
